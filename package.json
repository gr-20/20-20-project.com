{
  "name": "20-20-project.com",
  "version": "1.0.0",
  "description": "Website for 20-20-project.com",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "webpack --mode=production",
    "start": "webpack-dev-server --open",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint src/**/*.js",
    "lint": "concurrently \"npm run lint:js\" \"npm run lint:css\"",
    "predeploy": "cp src/CNAME build/",
    "deploy": "gh-pages -d build -m \"Publishing GitHub Pages [skip ci]\""
  },
  "engines": {
    "node": ">=10.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gr-20/20-20-project.com.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gr-20/20-20-project.com/issues"
  },
  "homepage": "/",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-prettier": "^1.0.6",
    "sugarss": "^2.0.0",
    "terser-webpack-plugin": "^1.2.3",
    "topbar": "^0.1.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "hyperapp": "^1.2.9",
    "purecss": "^1.0.0"
  },
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "inlineCss": false,
    "minifyHtml": {
      "collapseWhitespace": false
    }
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-preset-env": {},
      "postcss-import": {},
      "postcss-nested": {},
      "postcss-for": {}
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 80
  },
  "stylelint": {
    "extends": [
      "stylelint-prettier/recommended"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2016,
      "sourceType": "module"
    }
  }
}
