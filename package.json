{
  "name": "20-20-project.com",
  "version": "1.0.0",
  "description": "Website for 20-20-project.com",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "webpack --mode=production",
    "watch": "npm run build -- --watch",
    "start": "webpack-dev-server --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gr-20/20-20-project.com.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gr-20/20-20-project.com/issues"
  },
  "homepage": "https://github.com/gr-20/20-20-project.com#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^6.5.0",
    "style-loader": "^0.23.1",
    "sugarss": "^2.0.0",
    "terser-webpack-plugin": "^1.2.1",
    "topbar": "^0.1.3",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "hyperapp": "^1.2.9",
    "purecss": "^1.0.0"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {},
      "cssnano": {}
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2016,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
